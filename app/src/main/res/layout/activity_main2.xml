<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/titulo_cadastro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastro"
        android:textSize="48sp"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="95dp" />

    <EditText
        android:id="@+id/cpf_t2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="CPF:"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="264dp" />

    <EditText
        android:id="@+id/email_t2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="E-mail:"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="399dp" />

    <EditText
        android:id="@+id/nome_t2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Nome:"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="201dp" />

    <EditText
        android:id="@+id/telefone_t2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Telefone:"
        tools:layout_editor_absoluteX="100dp"
        tools:layout_editor_absoluteY="330dp" />

    <Button
        android:id="@+id/cadastro_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastrar-se"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="486dp" />

    <Button
        android:id="@+id/voltar_bt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voltar"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="566dp" />

</androidx.constraintlayout.widget.ConstraintLayout>